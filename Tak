    -- ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡∏™‡∏£‡πâ‡∏≤‡∏á‡∏õ‡∏∏‡πà‡∏°
    local function createButton(name, url, isCategory)
        local btn = Instance.new("TextButton")
        btn.Parent = FunctionButtonsFrame
        btn.Size = UDim2.new(1, -10, 0, isCategory and 30 or 40)
        btn.BackgroundColor3 = isCategory and Color3.fromRGB(35, 35, 50) or Color3.fromRGB(50, 50, 80)
        btn.Font = Enum.Font.GothamBold
        btn.TextSize = isCategory and 16 or 14
        btn.TextColor3 = isCategory and Color3.fromRGB(255, 215, 0) or Color3.fromRGB(240, 240, 240)
        btn.TextXAlignment = Enum.TextXAlignment.Left
        btn.Text = name
        btn.AutoButtonColor = not isCategory

        Instance.new("UICorner", btn).CornerRadius = UDim.new(0, 8)
        local stroke = Instance.new("UIStroke", btn)
        stroke.Thickness = 1.5
        stroke.Color = Color3.fromRGB(0, 0, 0)
        stroke.Transparency = 0.3

        if not isCategory then
            btn.MouseEnter:Connect(function()
                btn.BackgroundColor3 = Color3.fromRGB(70, 70, 120)
            end)
            btn.MouseLeave:Connect(function()
                btn.BackgroundColor3 = Color3.fromRGB(50, 50, 80)
            end)
            btn.MouseButton1Click:Connect(function()
                stroke.Transparency = 0
                TweenService:Create(stroke, TweenInfo.new(0.3), {Transparency = 0.3}):Play()
                loadstring(game:HttpGet(url))()
            end)
        end
    end

    -- ‡∏ß‡∏ô‡∏™‡∏£‡πâ‡∏≤‡∏á‡∏´‡∏°‡∏ß‡∏î‡∏´‡∏°‡∏π‡πà‡πÅ‡∏•‡∏∞‡∏õ‡∏∏‡πà‡∏°
    for category, scripts in pairs(functionCategories) do
        createButton(category, nil, true)
        for name, url in pairs(scripts) do
            createButton("   " .. name, url, false)
        end
    end

    -- ‡∏õ‡∏∏‡πà‡∏° Toggle
    local Toggle = Instance.new("TextButton")
    Toggle.Parent = ScreenGui
    Toggle.BackgroundColor3 = Color3.fromRGB(30, 30, 30)
    Toggle.Size = UDim2.new(0, 50, 0, 50)
    Toggle.Position = UDim2.new(0, 60, 0, 60)
    Toggle.Font = Enum.Font.GothamBold
    Toggle.Text = "W"
    Toggle.TextSize = 20
    Toggle.TextColor3 = Color3.fromRGB(255, 255, 255)
    Instance.new("UICorner", Toggle).CornerRadius = UDim.new(1, 0)
    local ToggleStroke = Instance.new("UIStroke", Toggle)
    ToggleStroke.Color = Color3.fromRGB(0, 0, 0)
    ToggleStroke.Thickness = 2
    ToggleStroke.Transparency = 0.3

    local isVisible = true
    Toggle.MouseButton1Click:Connect(function()
        isVisible = not isVisible
        MainFrame.Visible = isVisible
    end)
end

createUI()

-- ‡∏ï‡∏£‡∏ß‡∏à‡∏™‡∏≠‡∏ö‡∏ã‡πâ‡∏≥‡∏´‡∏≤‡∏Å‡∏ñ‡∏π‡∏Å‡∏•‡∏ö
while true do
    wait(1)
    if not CoreGui:FindFirstChild("WACKShopUI") then
        createUI()
    end
end
